#include<stdio.h>
 int main()
 {
     int a[100][100],b[100][100],n,j,i;

     printf("Ce dimensiune au matricile?\n");
     scanf("%d",&n);

     printf("Introduceti prima matrice:\n");
     for(i=1;i<=n;i++)
      for(j=1;j<=n;j++)
       scanf("%d",&a[i][j]);

     printf("Introduceti a doua matrice:\n");
     for(i=1;i<=n;i++)
      for(j=1;j<=n;j++)
       scanf("%d",&b[i][j]);

     printf("Matricea rezultata in urma sumei:");
     for(i=1;i<=n;i++)
     {
      printf("\n");
      for(j=1;j<=n;j++)
       printf("%d ",a[i][j]+b[i][j]);
     }
     printf("\n");

     return 0;
}

